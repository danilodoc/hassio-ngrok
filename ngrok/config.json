{
  "name": "ngrok Client",
  "version": "dev",
  "slug": "ngrok",
  "description": "A ngrok client for Hass.io",
  "webui": "[PROTO:ssl]://[HOST]:[PORT:4040]",
  "ingress": true,
  "url": "https://github.com/ThePicklenat0r/hassio-ngrok",
  "arch": [
    "aarch64",
    "amd64",
    "armhf",
    "armv7",
    "i386"
  ],
  "startup": "system",
  "boot": "auto",
  "auth_api": true,
  "ports": {
    "4040/tcp": "4040"
  },
  "ports_description": {
    "4040/tcp": "Web interface"
  },
  "options": {
    "log_level": "info",
    "ssl": false,
    "certfile": "fullchain.pem",
    "keyfile": "privkey.pem",
    "auth_token": "",
    "region": "us",
    "tunnels":
      [
        {
          "name": "hass",
          "proto": "http",
          "addr": 8123,
          "inspect": false
        }
      ]
  },
  "schema": {
    "log_level": "list(crit|warn|error|info|debug)$)",
    "ssl": "bool",
    "certfile": "str",
    "keyfile": "str",
    "auth_token": "str?",
    "region": "list(us|eu|ap|au|sa|jp|in)?",
    "tunnels":
    [
      {
        "name": "str",
        "proto": "list(http|tls|tcp)",
        "addr": "match(^((([0-9]|[1-9][0-9]|1[0-9]{2}|2[0-4][0-9]|25[0-5])\\.){3}([0-9]|[1-9][0-9]|1[0-9]{2}|2[0-4][0-9]|25[0-5]):([1-9]|[1-5]?[0-9]{2,4}|6[1-4][0-9]{3}|65[1-4][0-9]{2}|655[1-2][0-9]|6553[1-5]))|([1-9]|[1-5]?[0-9]{2,4}|6[1-4][0-9]{3}|65[1-4][0-9]{2}|655[1-2][0-9]|6553[1-5])$)",
        "inspect": "bool?",
        "auth": "str?",
        "host_header": "str?",
        "bind_tls": "bool?",
        "subdomain": "str?",
        "hostname": "str?",
        "crt": "str?",
        "key": "str?",
        "client_cas": "str?",
        "remote_addr": "match(^(([0-9]|[1-9][0-9]|1[0-9]{2}|2[0-4][0-9]|25[0-5])\\.){3}([0-9]|[1-9][0-9]|1[0-9]{2}|2[0-4][0-9]|25[0-5]):([1-9]|[1-5]?[0-9]{2,4}|6[1-4][0-9]{3}|65[1-4][0-9]{2}|655[1-2][0-9]|6553[1-5])$)?",
        "metadata": "str?",
        "leave_front_door_open": "bool?"
      }
    ]
  }
}
